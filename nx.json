{
  "npmScope": "cool-company",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "cool-todo": {
      "tags": []
    },
    "cool-todo-backend": {
      "tags": []
    },
    "cool-todo-e2e": {
      "tags": [],
      "implicitDependencies": ["cool-todo"]
    },
    "feature-todos-list": {
      "tags": ["feature"]
    },
    "ui-todos-list": {
      "tags": ["ui"]
    }
  }
}
